  συνάρτηση αύξηση(α,β)
    διαπροσωπεία
    είσοδος α
    έξοδος β
    αρχή_συνάρτησης
      β := α + 1 ;
      αύξηση := α + 1    { δεν μπαίνει ερωτηματικό
      είναι τέλος block }
    τέλος_συνάρτησης
  
        α := 1 ;
        β := 2 + α * α / (2 - α - (2*α))
        
  τέλος_προγράμματος
